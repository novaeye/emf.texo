      </orm:basic>
      <orm:basic name="amount" optional="false">
        <orm:column name="amount"/>
      </orm:basic>
      <orm:many-to-one name="debitAccount" optional="false">
        <orm:join-column name="accounting_JournalStatement_debitAccount"/>
