      </orm:many-to-one>
      <orm:many-to-one name="creditAccount" optional="false">
        <orm:join-column name="accounting_JournalStatement_creditAccount"/>
        <orm:cascade>
          <orm:cascade-persist/>
          <orm:cascade-merge/>
